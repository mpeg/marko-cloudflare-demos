{
    "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
    "name": "marko-cloudflare-demo",
    "compatibility_date": "2025-10-11",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "main": "src/worker-entry.ts",
    "assets": {
        "directory": "./dist/public",
        "binding": "ASSETS",
    },
    "kv_namespaces": [
        {
            "binding": "CACHE",
            "id": "xx"
        }
    ]
}